    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thelittleteaclipper.habitune"
    },
    "web": {
      "favicon": "src/assets/images/favicon.png"
    },
    "sourceExts": [
      "js",
      "json",
      "ts",
      "tsx",
      "jsx",
      "vue"
    ],
    "extra": {
      "eas": {
        "projectId": "e6fc2f7a-9fc8-4346-8982-3b5e31034c64"
      }
    }
  }
}
