{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/api/dist/server.js",
            "outFiles": [
                "${workspaceFolder}/api/dist/**/*.js"
            ],
            "preLaunchTask": "npm: build - api",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}