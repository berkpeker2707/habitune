{
  "name": "googlebackendexample",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "rimraf dist && tsc && npm run copyLogs && npm run copyHTML && npm run copyStatic",
    "copyLogs": "copyfiles logs/** dist",
    "copyHTML": "copyfiles view/** dist",
    "copyStatic": "copyfiles public/** dist",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "api": "nodemon server.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "start:dev": "tsc-watch --onSuccess 'node --inspect=0.0.0.0:5858 dist/src/server.js'",
    "json-server": "json-server --watch db.json"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.1.4",
    "jsonwebtoken": "^9.0.0",
    "mongodb-memory-server": "^8.12.2",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth2": "^0.1.5",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.3"
  }
}
