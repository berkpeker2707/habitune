{
  "name": "habitune_api",
  "version": "1.7.2",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "rimraf dist && tsc && npm run copyHTML && npm run copyStatic && npm run copyRobots",
    "copyLogs": "copyfiles 'logs/**' dist",
    "copyHTML": "copyfiles 'view/**' dist",
    "copyStatic": "copyfiles 'public/**' dist",
    "copyRobots": "copyfiles robots.txt dist",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "lint": "npx eslint **/*.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --watchAll",
    "api": "nodemon server.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "start:dev": "tsc-watch --onSuccess 'node --inspect=0.0.0.0:5858 dist/src/server.js'"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "connect-mongodb-session": "^3.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-form-data": "^2.0.22",
    "express-rate-limit": "^7.1.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.11.0",
    "helmet": "^6.1.4",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-form-data": "^2.0.5",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth2": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.57.0",
    "globals": "^15.1.0",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.3",
    "typescript-eslint": "^7.8.0"
  }
}
